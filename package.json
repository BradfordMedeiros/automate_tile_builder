    "mqtt": "^2.3.0",
    "react": "^15.4.2",
    "react-color": "^2.13.8",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
